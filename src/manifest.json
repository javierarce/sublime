{
  "$schema": "https://raw.githubusercontent.com/BohemianCoding/SketchAPI/develop/docs/sketch-plugin-manifest-schema.json",
    "icon": "icon.png",
    "commands": [{
      "script": "./command.js",
      "handlers": {
        "onDocumentChanged": "onDocumentChanged"
      }
    }, {
      "name": "Activar",
      "description": "On",
      "identifier": "sublime.on",
      "handler": "on",
      "script": "./command.js"
    }, {
      "name": "Desactivar",
      "description": "Off",
      "identifier": "sublime.off",
      "handler": "off",
      "script": "./command.js"
    }],
    "menu": {
      "title": "Sublime",
      "items": [
        "sublime.on",
        "sublime.off"
      ]
    }
}
